{
	"name": "n8n-nodes-security-gate",
	"version": "0.1.1",
	"description": "n8n node for visual and secure credential validation",
	"license": "MIT",
	"homepage": "",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Hernan Hegykozi",
		"email": "hernanjh@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hernanjh/n8n-nodes-security-gate.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/SecurityTokenApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/SecurityGate/SecurityGate.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/node": "^25.1.0",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
